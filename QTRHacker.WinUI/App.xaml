<?xml version="1.0" encoding="utf-8"?>
<Application
    x:Class="QTRHacker.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:QTRHacker.Converters"
    xmlns:local="using:QTRHacker">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ms-appx:///Styles/TextBox.xaml"/>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
                <!-- Other merged dictionaries here -->
            </ResourceDictionary.MergedDictionaries>
            <!-- Other app resources here -->
            <converters:BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
            <converters:ItemTypeToImageConverter x:Key="ItemTypeToImageConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
